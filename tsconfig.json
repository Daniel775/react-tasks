{
	"compilerOptions": {
		"target": "ES6",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"module": "ESNext",
		"sourceMap": true,
		"outDir": "dist",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": ["dist", "node_modules", "src/**/*.stories.tsx"]
}
